{
  "name": "@web-builder/frontend",
  "version": "1.0.0",
  "scripts": {
    "build:dev": "NODE_ENV=development npm-run-all clean build:dev:*",
    "build:dev:client": "yarn webpack --config=devtools/webpack/webpack.config.ts --env mode=development --env presets=client",
    "build:dev:server": "yarn webpack --config=devtools/webpack/webpack.config.ts --env mode=development --env presets=server",
    "build:production": "NODE_ENV=production npm-run-all clean build:production:*",
    "build:production:client": "yarn webpack --config=devtools/webpack/webpack.config.ts --env mode=production --env presets=client",
    "build:production:server": "yarn webpack --config=devtools/webpack/webpack.config.ts --env mode=production --env presets=server",
    "clean": "rimraf dist",
    "dev": "DEBUG=server:*,express:* npm-run-all clean serve:dev",
    "serve:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register devtools/devServer.ts",
    "start": "DEBUG=server:* node dist/server.js",
    "test": "jest --no-coverage",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "author": "Carlos Santana",
  "license": "MIT",
  "dependencies": {
    "design-system": "1.0.0",
    "@apollo/client": "^3.5.10",
    "@contentpi/lib": "^1.0.10",
    "@contentpi/ui-kit": "^2.0.9",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "html-webpack-plugin": "^5.5.0",
    "ip": "^1.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.2",
    "run-script-webpack-plugin": "^0.0.11",
    "serialize-javascript": "^6.0.0",
    "styled-components": "^5.3.3",
    "tsconfig-paths": "^3.14.0",
    "typescript-plugin-styled-components": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "webpackbar": "^5.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/ip": "^1.1.0",
    "@types/serialize-javascript": "^5.0.2",
    "@types/styled-components": "^5.1.24",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-env": "^1.16.3",
    "@types/webpack-node-externals": "^2.5.3",
    "@web-builder/design-system": "1.0.0",
    "buffer": "^6.0.3",
    "dotenv-webpack": "^7.1.0",
    "file-loader": "^6.2.0",
    "jest": "^27.5.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "main": "index.js",
  "keywords": [],
